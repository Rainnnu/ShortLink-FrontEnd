button\r\n              @click=\"sendCode\"\r\n              class=\"bg-blue-500 text-white rounded p-2\"\r\n              >发送验证码</el-button\r\n            >\r\n          </div>\r\n        </el-form-item>\r\n        <el-form-item class=\"flex justify-between space-x-2\">\r\n          <el-button\r\n            type=\"primary\"\r\n            @click=\"login\"\r\n            class=\"bg-blue-500 text-white rounded p-2 w-full\"\r\n            >登录</el-button\r\n          >\r\n          <el-button\r\n            @click=\"toggleLoginType\"\r\n            class=\"bg-gray-300 text-gray-700 rounded p-2 w-full\"\r\n          >\r\n            切换到 {{ isPasswordLogin ? \"验证码登录\" : \"密码登录\" }}\r\n          </el-button>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button\r\n            @click=\"forgotPassword\"\r\n            class=\"text-blue-500 hover:underline\"\r\n            >忘记密码</el-button\r\n          >\r\n        </el-form-item>\r\n      </el-form>\r\n      <!-- 忘记密码页面 -->\r\n      <div v-if=\"isForgotPasswordPage\" class=\"transition-opacity duration-300\">\r\n        <h2 class=\"text-2xl font-bold text-center mb-6\">忘记密码</h2>\r\n        <el-form\r\n          :model=\"forgotPasswordForm\"\r\n          ref=\"forgotPasswordFormRef\"\r\n          label-width=\"80px\"\r\n          class=\"space-y-4\"\r\n        >\r\n          <el-form-item label=\"邮箱\">\r\n            <el-input\r\n              v-model=\"forgotPasswordForm.email\"\r\n              class=\"border border-gray-300 rounded p-2 w-full\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"验证码\">\r\n            <div class=\"flex space-x-2\">\r\n              <el-input\r\n                v-model=\"forgotPasswordForm.code\"\r\n                class=\"border border-gray-300 rounded p-2 w-full\"\r\n              ></el-input>\r\n              <el-button\r\n                @click=\"sendVerifyCode\"\r\n                class=\"bg-blue-500 text-white rounded p-2